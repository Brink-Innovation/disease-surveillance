      <bind nodeset="/cholera_home_visit/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/cholera_home_visit/inputs/source" type="string"/>
      <bind nodeset="/cholera_home_visit/inputs/source_id" type="string"/>
      <bind nodeset="/cholera_home_visit/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/cholera_home_visit/inputs/contact/name" type="string"/>
      <bind nodeset="/cholera_home_visit/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/cholera_home_visit/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/cholera_home_visit/inputs/contact/sex" type="string"/>
      <bind nodeset="/cholera_home_visit/inputs/contact/contact/name" type="string"/>
      <bind nodeset="/cholera_home_visit/inputs/contact/contact/phone" type="string"/>
      <bind nodeset="/cholera_home_visit/source" type="string" calculate="../inputs/source"/>
      <bind nodeset="/cholera_home_visit/source_id" type="string" calculate="../inputs/source_id"/>
      <bind nodeset="/cholera_home_visit/patient_age_in_years" type="string" calculate="floor( difference-in-months( ../inputs/contact/date_of_birth, today() ) div 12 )"/>
      <bind nodeset="/cholera_home_visit/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/cholera_home_visit/patient_id" type="string" required="true()" calculate="../inputs/contact/_id"/>
      <bind nodeset="/cholera_home_visit/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/cholera_home_visit/date_of_birth" type="string" calculate="../inputs/contact/date_of_birth"/>
      <bind nodeset="/cholera_home_visit/treatment_adherance/taking_treatment" type="select1"/>
      <bind nodeset="/cholera_home_visit/treatment_adherance/challenges_taking_medication" type="select1"/>
      <bind nodeset="/cholera_home_visit/treatment_adherance/n_provide_emotional_support" readonly="true()" type="string" relevant="selected(../challenges_taking_medication, 'yes')"/>
      <bind nodeset="/cholera_home_visit/treatment_adherance/next_visit" type="date" jr:constraintMsg="jr:itext('/cholera_home_visit/treatment_adherance/next_visit:jr:constraintMsg')" constraint="decimal-date-time(.) &gt;= decimal-date-time(today())"/>
      <bind nodeset="/cholera_home_visit/treatment_adherance/n_plan_to_accompany" readonly="true()" type="string"/>
      <bind nodeset="/cholera_home_visit/signs_screening/signs" type="select" jr:constraintMsg="jr:itext('/cholera_home_visit/signs_screening/signs:jr:constraintMsg')" constraint="not(selected(., 'none')
and count-selected(.) &gt; 1)"/>
      <bind nodeset="/cholera_home_visit/signs_screening/refer_and_accompany" type="string" calculate="if(selected(../signs, 'profuse_watery_diarrhea')
or selected(../signs, 'dehydration')
or selected(../signs, 'lack_of_appetite')
or selected(../signs, 'vomits_everything')
or selected(../signs, 'body_restlessnes')
or selected(../signs, 'leg_cramps'),
'yes', 'no')"/>
      <bind nodeset="/cholera_home_visit/signs_screening/refer_only" type="string" calculate="if(selected(../signs, 'dry_skin')
or selected(../signs, 'fever')
and ../refer_and_accompany = 'no',
'yes', 'no')"/>
      <bind nodeset="/cholera_home_visit/signs_screening/n_refer_and_accompany1" readonly="true()" type="string" relevant="../refer_and_accompany = 'yes'"/>
      <bind nodeset="/cholera_home_visit/signs_screening/n_refer1" readonly="true()" type="string" relevant="../refer_only = 'yes'
and ../refer_and_accompany = 'no'"/>
      <bind nodeset="/cholera_home_visit/contact_tracing/other_family_members" type="select1"/>
      <bind nodeset="/cholera_home_visit/contact_tracing/n_educate" readonly="true()" type="string" relevant="selected(../other_family_members, 'yes')"/>
      <bind nodeset="/cholera_home_visit/ref_type" type="select1" relevant="(not(selected(../signs_screening/signs, 'none'))
and count-selected(../signs_screening/signs) &gt;= 1)"/>
      <bind nodeset="/cholera_home_visit/group_review/n_submit" readonly="true()" type="string"/>
      <bind nodeset="/cholera_home_visit/group_review/n_patient_details_title" readonly="true()" type="string"/>
      <bind nodeset="/cholera_home_visit/group_review/n_patient_details" readonly="true()" type="string"/>
      <bind nodeset="/cholera_home_visit/group_review/n_findings_title" readonly="true()" type="string"/>
      <bind nodeset="/cholera_home_visit/group_review/n_adhering_to_treatment" readonly="true()" type="string" relevant="selected(../../treatment_adherance/taking_treatment, 'yes')"/>
      <bind nodeset="/cholera_home_visit/group_review/n_not_adhering_to_treatment" readonly="true()" type="string" relevant="selected(../../treatment_adherance/taking_treatment, 'no')"/>
      <bind nodeset="/cholera_home_visit/group_review/n_signs_title" readonly="true()" type="string"/>
      <bind nodeset="/cholera_home_visit/group_review/n_profuse_watery_diarrhea" readonly="true()" type="string" relevant="selected(../../signs_screening/signs, 'profuse_watery_diarrhea')"/>
      <bind nodeset="/cholera_home_visit/group_review/n_dehydration" readonly="true()" type="string" relevant="selected(../../signs_screening/signs, 'dehydration')"/>
      <bind nodeset="/cholera_home_visit/group_review/n_lack_of_appetite" readonly="true()" type="string" relevant="selected(../../signs_screening/signs, 'lack_of_appetite')"/>
      <bind nodeset="/cholera_home_visit/group_review/n_vomits_everything" readonly="true()" type="string" relevant="selected(../../signs_screening/signs, 'vomits_everything')"/>
      <bind nodeset="/cholera_home_visit/group_review/n_body_restlessnes" readonly="true()" type="string" relevant="selected(../../signs_screening/signs, 'body_restlessnes')"/>
      <bind nodeset="/cholera_home_visit/group_review/n_leg_cramps" readonly="true()" type="string" relevant="selected(../../signs_screening/signs, 'leg_cramps')"/>
      <bind nodeset="/cholera_home_visit/group_review/n_dry_skin" readonly="true()" type="string" relevant="selected(../../signs_screening/signs, 'dry_skin')"/>
      <bind nodeset="/cholera_home_visit/group_review/n_fever" readonly="true()" type="string" relevant="selected(../../signs_screening/signs, 'fever')"/>
      <bind nodeset="/cholera_home_visit/group_review/n_none" readonly="true()" type="string" relevant="selected(../../signs_screening/signs, 'none')"/>
      <bind nodeset="/cholera_home_visit/group_review/n_tracing_title" readonly="true()" type="string" relevant="selected(../../contact_tracing/other_family_members, 'yes')"/>
      <bind nodeset="/cholera_home_visit/group_review/household_referred" type="select1" relevant="selected(../../contact_tracing/other_family_members, 'yes')"/>
      <bind nodeset="/cholera_home_visit/group_review/n_follow_up_title" readonly="true()" type="string"/>
      <bind nodeset="/cholera_home_visit/group_review/n_follow_up" readonly="true()" type="string"/>
      <bind nodeset="/cholera_home_visit/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
